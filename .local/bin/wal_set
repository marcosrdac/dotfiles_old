#!/bin/bash

toggle_untoggle_hidden ()
{
 for i in "$@"
 do
 bspwm_toggle_hidden $i
 bspwm_toggle_hidden $i
 done
}


if [ \( -L "$1" \) ]
then
    file=$(readlink $1)
else
    file=$1
fi


toggle_untoggle_hidden dropdown_terminal dropdown_mail dropdown_calculator dropdown_music_player
wpg -a $file
wpg -A $file
wpg -s $file
dunst_start
qutebrowser_reload.py
#wal_steam -w
notify-send "New theme set!"
