#!/bin/bash

pidof dunst && killall dunst

dunst &

notify-send "message one"
notify-send "message two"
notify-send "this message is too long to fit the normal width of a dunst cell"
notify-send "GEEEEEEEEEEEEEEEZZ IIIIIIIIIIIITS REEEEEEEALY REEEEAALY BIIIIIIIIIIIIIIIIG"
notify-send "message five"
notify-send "last message"
