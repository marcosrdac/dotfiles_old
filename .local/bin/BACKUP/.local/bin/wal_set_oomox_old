#!/bin/bash

colorz=/usr/lib/python3.7/site-packages/pywal/backends/colorz.py
oomox_change_color=/opt/oomox/plugins/theme_oomox/gtk-theme/change_color.sh


wal -q --backend $colorz -i $1
dunst_start
killall i3bar
i3-msg reload
i3-msg exec "i3bar --bar_id=bar-0"
$oomox_change_color -o wal $HOME/.config/wal/cache/colors-oomox
qutebrowser_reload.py
wal-steam -w
notify-send "New theme set!"

#feh_file=$(cat ~/.fehbg)
#wallpaper=$(echo $feh_file | sed "s/.*'\(.*\).*'/\1/")
